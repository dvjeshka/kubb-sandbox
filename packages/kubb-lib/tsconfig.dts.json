{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@apiClient": ["./apiClient.ts"]
    },
    "allowJs": true,
    "checkJs": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "allowImportingTsExtensions": true,
    "outDir": "dist",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2020",
    "strict": false
  },
  "include": [
    "generated",
    "helpers"
  ]
}