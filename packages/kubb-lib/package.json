{
  "name": "kubb-lib",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/generated/index.cjs",
  "module": "./dist/generated/index.js",
  "types": "./dist/generated/index.d.ts",
  "scripts": {
    "generate": "kubb generate",
    "dev": "vite",
    "build": "rollup -c",
    "_postbuild": "ts-node ./scripts/generate-barrel-dts.ts",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@kubb/cli": "^4.9.2",
    "@kubb/core": "^4.9.2",
    "@kubb/plugin-client": "^4.9.2",
    "@kubb/plugin-oas": "^4.9.2",
    "@kubb/plugin-react-query": "^4.9.2",
    "@kubb/plugin-swr": "^4.9.2",
    "@kubb/plugin-ts": "^4.9.2",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "glob": "^13.0.0",
    "openapi-format": "^1.25.2",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.3.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "use-sync-external-store": "^1.6.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/generated/index.d.ts",
        "default": "./dist/generated/index.js"
      },
      "require": {
        "types": "./dist/generated/index.d.ts",
        "default": "./dist/generated/index.cjs"
      }
    },
    "./types/*": {
      "import": {
        "types": "./dist/generated/types/*.d.ts",
        "default": "./dist/generated/types/*.js"
      },
      "require": {
        "types": "./dist/generated/types/*.d.ts",
        "default": "./dist/generated/types/*.cjs"
      }
    },
    "./hooks/*": {
      "import": {
        "types": "./dist/generated/hooks/*.d.ts",
        "default": "./dist/generated/hooks/*.js"
      },
      "require": {
        "types": "./dist/generated/hooks/*.d.ts",
        "default": "./dist/generated/hooks/*.cjs"
      }
    },
    "./clients/*": {
      "import": {
        "types": "./dist/generated/clients/*.d.ts",
        "default": "./dist/generated/clients/*.js"
      },
      "require": {
        "types": "./dist/generated/clients/*.d.ts",
        "default": "./dist/generated/clients/*.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/generated/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}
