{
  "name": "kubb-lib",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build:js": "rollup -c --environment BUILD:js",
    "build:dts": "tsc -p tsconfig.dts.json",
    "build": "npm run build:js && npm run build:dts",
    "generate": "kubb generate",
    "dev": "vite",
    "_build": "rollup -c",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@kubb/cli": "^4.9.2",
    "@kubb/core": "^4.9.2",
    "@kubb/plugin-client": "^4.9.2",
    "@kubb/plugin-oas": "^4.9.2",
    "@kubb/plugin-react-query": "^4.9.2",
    "@kubb/plugin-swr": "^4.9.2",
    "@kubb/plugin-ts": "^4.9.2",
    "@rollup/plugin-typescript": "^12.3.0",
    "glob": "^13.0.0",
    "openapi-format": "^1.25.2",
    "rollup": "^4.53.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "exports": {
    "./apiClient": {
      "types": "./dist/apiClient.d.ts"
    },
    "./types/*": {
      "import": {
        "types": "./dist/types/*.d.ts"
      },
      "require": {
        "types": "./dist/types/*.d.ts",
        "default": "./dist/types/*.cjs"
      }
    },
    "./hooks/*": {
      "import": {
        "types": "./dist/hooks/*.d.ts",
        "default": "./dist/hooks/*.js"
      },
      "require": {
        "types": "./dist/hooks/*.d.ts",
        "default": "./dist/hooks/*.cjs"
      }
    },
    "./api/*": {
      "import": {
        "types": "./dist/api/*.d.ts",
        "default": "./dist/api/*.js"
      },
      "require": {
        "types": "./dist/api/*.d.ts",
        "default": "./dist/api/*.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}
